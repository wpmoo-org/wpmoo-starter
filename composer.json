{
  "name": "wpmoo/wpmoo-starter",
  "description": "Starter plugin demonstrating WPMoo Framework usage.",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=7.4",
    "wpmoo-org/wpmoo": "^0.2.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../../../wpmoo-org/wpmoo",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "WPMooStarter\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "moo": "php vendor/bin/moo"
  }
}
