includes:
    - vendor/szepeviktor/phpstan-wordpress/extension.neon
    - phpstan-baseline.neon

parameters:
    level: 6
    phpVersion: 70400
    paths:
        - src
    excludePaths:
        - vendor
        - tests
    # Load Field builder magic static constructors from framework vendor path.
    stubFiles:
        - phpstan-stubs/wpmoo-fields-field.stub.php
    reportUnmatchedIgnoredErrors: false
    treatPhpDocTypesAsCertain: false
